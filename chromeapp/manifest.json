{
  "manifest_version": 2,
  "name": "Smart To Do List",
  "version": "1",
  "minimum_chrome_version": "23",
  "icons": {
    "16": "icon_16.png",
    "128": "icon_128.png"
  },
  "app": {
    "background": {
      "scripts": ["main.js"]
    }
  },
  "permissions":["storage"],
  "sandbox": {
    "pages": ["data.html"]
  },
  "incognito":"split"
}
